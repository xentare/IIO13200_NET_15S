<Window x:Class="Tehtävä5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tehtävä5"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnGetDataTable" Content="Hae läsnäolot (DataTable)" Margin="10,38,10,250" Click="btnGetDataTable_Click"/>
        <DataGrid x:Name="dgData" Margin="10,152,10,10" ItemsSource="{Binding}"/>
        <TextBox x:Name="asioidTextBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="254"/>
    </Grid>
</Window>
